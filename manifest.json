{
	"name": "xhr2md",
	"description": "Record XMLHttpRequest and parse it to markdown in API blueprint format.",
	"version": "0.1",
	"manifest_version": 2,
	"minimum_chrome_version": "18.0",
	"icons": {
		"24": "gfx/icon_24.png",
		"128": "gfx/icon_128.png"
	},
	"devtools_page": "devtools.html",
	"permissions": [
		"tabs",
		"clipboardWrite",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"content_security_policy": "script-src 'unsafe-eval' 'self' https://apis.google.com https://platform.twitter.com https://www.facebook.com; object-src 'self'"
}